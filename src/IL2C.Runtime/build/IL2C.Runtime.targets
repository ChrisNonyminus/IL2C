<Project ToolVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IL2CNativeIncludeDir Condition="'$(IL2CNativeIncludeDir)' != ''">$(IL2CRuntimeBasePath)/include;$(IL2CNativeIncludeDir)</IL2CNativeIncludeDir>
    <IL2CNativeIncludeDir Condition="'$(IL2CNativeIncludeDir)' == ''">$(IL2CRuntimeBasePath)/include</IL2CNativeIncludeDir>
    <IL2CNativeLibPath Condition="'$(IL2CNativeLibPath)' != ''">$(IL2CRuntimeBasePath)/lib/$(IL2CLibConfiguration)/libil2c-gcc4-win-mingw32.a;$(IL2CNativeLibPath)</IL2CNativeLibPath>
    <IL2CNativeLibPath Condition="'$(IL2CNativeLibPath)' == ''">$(IL2CRuntimeBasePath)/lib/$(IL2CLibConfiguration)/libil2c-gcc4-win-mingw32.a</IL2CNativeLibPath>
    <IL2CNativeMainTemplatePath Condition="'$(IL2CNativeMainTemplatePath)' == ''">$(_IL2C_StandardNativeMainTemplatePath)</IL2CNativeMainTemplatePath>
  </PropertyGroup>
</Project>
