<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <IL2CNativeCompilerFlags Condition="'$(IL2CConfiguration)' == 'Debug'">-O0 -D_DEBUG</IL2CNativeCompilerFlags>
    <IL2CNativeCompilerFlags Condition="'$(IL2CConfiguration)' == 'Release'">-Ofast -fomit-frame-pointer -march=native -mtune=native -DNDEBUG</IL2CNativeCompilerFlags>
  </PropertyGroup>
</Project>
