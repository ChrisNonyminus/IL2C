<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(OS)' == 'Windows_NT'">
    <IL2CNativeCompiler>$(MSBuildThisFileDirectory)..\gcc4\bin\gcc.exe</IL2CNativeCompiler>
    <IL2CNativeCompilerRequiredFlags>-D_WIN32 -Wl,--enable-stdcall-fixup -Wl,--add-stdcall-alias</IL2CNativeCompilerRequiredFlags>
    <IL2CNativeArciver>$(MSBuildThisFileDirectory)..\gcc4\bin\ar.exe</IL2CNativeArciver>
    <_IL2C_WinExeMainTemplatePath>$(MSBuildThisFileDirectory)..\src\main_winexe_template.c</_IL2C_WinExeMainTemplatePath>
  </PropertyGroup>
</Project>
